var request = require('request');
var fs=require('fs');
var query = 'english';
var url = 'https://en.wikipedia.org/wiki/Nodejs';
request(url, function (err, response, body) {
if(err){
var error = 'cannot connect to the server';
console.log(error);
} else {
fs.writeFileSync("Nodejs.html",body);
console.log('body:', body);
}
});
